        <em:id>moedict-addon-app@xuldict.3du.tw</em:id>
        <em:name>MoeDict Desktop Addon - Moedict-App</em:name>
        <em:version>1.2.1</em:version>
        <em:description>MoeDict Desktop App - Moedict-App</em:description>
        <em:creator>Rack Lin(racklin@gmail.com)</em:creator>
